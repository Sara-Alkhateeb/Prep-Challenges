            "source": [
                "function findMostFrequent(arr) {\n    let freq = {};          // Create an empty object to keep track of frequency counts\n    let maxFreq = 0;        // Initialize the maximum frequency count to zero\n    let mostFreqNum = null; // Initialize the most frequent number to null\n  \n    // Iterate over the array to update frequency counts\n    for (let num of arr) {\n      freq[num] = (freq[num] || 0) + 1; // Increment the count for the current number\n      if (freq[num] > maxFreq) {\n        // If the current number has a higher frequency than the current max, update the max\n        maxFreq = freq[num];\n        mostFreqNum = num;\n      }\n    }\n  \n    return mostFreqNum;  // Return the most frequent number\n  }\n  "
